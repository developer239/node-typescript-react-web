{
  "name": "node-typescript-react-web",
  "version": "0.0.1",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "lint:ts": "tslint './src/**/*.{ts,tsx}'",
    "lint:ts:fix": "tslint './src/**/*.{ts,tsx}' --fix",
    "lint:css": "node node_modules/.bin/stylelint ./src",
    "prettier": "prettier --write './src/**/*.{ts,tsx}'",
    "prod": "node node_modules/.bin/http-server build",
    "precommit": "lint-staged",
    "heroku-postbuild": "npm run build"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "formik": "^1.3.2",
    "http-server": "^0.11.1",
    "js-cookie": "^2.2.0",
    "nanoid": "^2.0.0",
    "ramda": "^0.26.1",
    "react": "^16.7.0-alpha.2",
    "react-dom": "^16.7.0-alpha.2",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.3",
    "serve": "^10.1.1",
    "styled-components": "^4.1.2",
    "styled-is": "^1.1.5",
    "yup": "^0.26.6"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/jest": "^23.3.10",
    "@types/js-cookie": "^2.2.0",
    "@types/nanoid": "^1.2.0",
    "@types/node": "^10.12.11",
    "@types/ramda": "^0.25.42",
    "@types/react": "^16.7.11",
    "@types/react-dom": "^16.0.11",
    "@types/react-router-dom": "^4.3.1",
    "@types/styled-components": "^4.1.2",
    "@types/webpack-env": "^1.13.6",
    "@types/yup": "^0.26.2",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.3",
    "stylelint": "^9.9.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.5.1",
    "tslint": "^5.11.0",
    "typescript": "3.2.4"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier  --write",
      "git add"
    ],
    "*.{json,css,md}": [
      "prettier  --write",
      "git add"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
